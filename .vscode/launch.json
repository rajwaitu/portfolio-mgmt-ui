{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "name": "Launch Server",
            "program": "${workspaceFolder}/serviceWorker.js",
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
            "runtimeExecutable": "C:/software/nodejs/node",
            "sourceMaps": false,
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                 "<node_internals>/**/*.js"
            ]
        }
       
    ]
}